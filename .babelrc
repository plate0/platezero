{
  "env": {
    "development": {
      "presets": [
        "next/babel"
      ],
      "plugins": [
        "dynamic-import-node",
        [
          "@babel/plugin-proposal-decorators",
          {
            "legacy": true
          },
        ]
      ]
    },
    "production": {
      "presets": [
        "next/babel"
      ],
      "plugins": [
        "dynamic-import-node",
        [
          "@babel/plugin-proposal-decorators",
          {
            "legacy": true
          }
        ]
      ]
    },
    "test": {
      "presets": [
        [
          "next/babel",
          {
            "preset-env": {
              "modules": "commonjs"
            }
          }
        ]
      ],
      "plugins": [
        "dynamic-import-node",
        [
          "@babel/plugin-proposal-decorators",
          {
            "legacy": true
          }
        ]
      ]
    }
  }
}
